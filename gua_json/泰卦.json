{
  "卦名": "泰卦",
  "卦辞": "元亨，利贞。",
  "象曰": "天地交，泰。后以财成天地之道，辅相天地之宜，以左右民。",
  "时运": "一切顺利，居安思危。",
  "财运": "买卖均宜，买入较佳。",
  "家宅": "家庭安稳；婚嫁大吉。",
  "身体": "柔软运动。",
  "动爻": [
    {
      "爻位": "初九",
      "爻辞": "拔茅，茹以其汇，征吉。",
      "象曰": "虽盘桓，志行正也；以贵下贱，大得民也。",
      "时运": "欲进不进，坚守正道。",
      "财运": "不利经商，可任用正人君子。",
      "家宅": "安居于贵宅；婚嫁吉祥。",
      "身体": "健康无虞。"
    },
    {
      "爻位": "九二",
      "爻辞": "见龙在田，利见大人。",
      "象曰": "见龙在田，德施普也。",
      "时运": "得贵人之助，有发展机会。",
      "财运": "开始涨价，官方采购。",
      "家宅": "婚姻大吉。",
      "身体": "运动健身。"
    },
    {
      "爻位": "九三",
      "爻辞": "君子终日乾乾，夕惕若；厉，无咎。",
      "象曰": "终日乾乾，反复道也。",
      "时运": "功名未显，戒慎免咎。",
      "财运": "日夜防范，可脱险获利。",
      "家宅": "勤俭保家；不宜攀结高亲。",
      "身体": "小心保养。"
    },
    {
      "爻位": "六四",
      "爻辞": "括囊，无咎无誉。",
      "象曰": "括囊无咎，慎不害也。",
      "时运": "收敛为要，但求无过。",
      "财运": "落袋为安，不再投资。",
      "家宅": "平安是福。",
      "身体": "谨慎保养。"
    },
    {
      "爻位": "六五",
      "爻辞": "黄裳，元吉。",
      "象曰": "黄裳元吉，文在中也。",
      "时运": "功名大显。",
      "财运": "必定获利。",
      "家宅": "和乐融融。",
      "身体": "病在肠胃。"
    },
    {
      "爻位": "上六",
      "爻辞": "亢龙有悔。",
      "象曰": "亢龙有悔，盈不可久也。",
      "时运": "由满招损，退而自省。",
      "财运": "过盈则必有亏。",
      "家宅": "婚嫁不利。",
      "身体": "命在旦夕。"
    }
  ]
}