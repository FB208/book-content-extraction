{
  "卦名": "小过卦",
  "卦辞": "亨。利贞。可小事，不可大事。飞鸟遗之音。不宜上，宜下，大吉。",
  "象曰": "山上有雷，小过。君子以行过乎恭，丧过乎哀，用过乎俭。",
  "时运": "清高自处，反遭人忌",
  "财运": "价格不合，无利可图",
  "家宅": "小心火灾",
  "身体": "郁火上升，慎用寒剂",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "飞鸟以凶。",
      "象曰": "飞鸟以凶，不可如何也。",
      "时运": "不安本分，结果堪虞",
      "财运": "不自量力，难免失败",
      "家宅": "屋宜低小；门当户对",
      "身体": "神魂难保"
    },
    {
      "爻位": "六二",
      "爻辞": "过其祖，遇其妣。不及其君，遇其臣。无咎。",
      "象曰": "不及其君，臣不可过也。",
      "时运": "平顺之运，得半已足",
      "财运": "虽未满载，得半亦可",
      "家宅": "不安之象；恐非正配",
      "身体": "药力未至，继续诊治"
    },
    {
      "爻位": "九三",
      "爻辞": "弗过防之，从或戕之，凶。",
      "象曰": "从或戕之，凶如何也？",
      "时运": "切勿躁进，自保免祸",
      "财运": "外出经营，盗贼可怕",
      "家宅": "谨防凶祸；小心冤家",
      "身体": "刀伤凶险"
    },
    {
      "爻位": "九四",
      "爻辞": "无咎，弗过遇之，往厉必戒。勿用，永贞。",
      "象曰": "弗过遇之，位不当也。往厉必戒，终不可长也。",
      "时运": "观察情势，不可妄动",
      "财运": "适可而止，切勿过贪",
      "家宅": "安居为宜；勿急媒聘",
      "身体": "静养心神"
    },
    {
      "爻位": "六五",
      "爻辞": "密云不雨，自我西郊。公弋取彼在穴。",
      "象曰": "密云不雨，已上也。",
      "时运": "平淡之时，难成大事",
      "财运": "小利可得，不必多想",
      "家宅": "小康之家；恐非正娶",
      "身体": "针灸可治"
    },
    {
      "爻位": "上六",
      "爻辞": "弗遇过之，飞鸟离之，凶。是谓灾眚。",
      "象曰": "弗遇过之，已亢也。",
      "时运": "不知退守，自取其祸",
      "财运": "时机不对，无得有失",
      "家宅": "离散之象；小心奸计",
      "身体": "凶险之病，速求良医"
    }
  ]
}