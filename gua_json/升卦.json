{
  "卦名": "升卦",
  "卦辞": "元亨，用见大人，勿恤，南征吉。",
  "象曰": "地中生木，升。君子以顺德，积小以高大。",
  "时运": "大地春回，日益高升。",
  "财运": "储蓄积财，可成富人。",
  "家宅": "改造大厦；以妾作嫡。",
  "身体": "肝火渐旺，早些调养。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "允升，大吉。",
      "象曰": "允升大吉，上合志也。",
      "时运": "名利双收，所求皆遂。",
      "财运": "货价涨升，大可获利。",
      "家宅": "乔迁之喜；两性好合。",
      "身体": "有病即治，不可拖延。"
    },
    {
      "爻位": "九二",
      "爻辞": "孚乃利用禴，无咎。",
      "象曰": "九二之孚，有喜也。",
      "时运": "正当好运，喜事临门。",
      "财运": "诚信经营，可保获利。",
      "家宅": "必有喜事；阴阳合德。",
      "身体": "最好祈神。"
    },
    {
      "爻位": "九三",
      "爻辞": "升虚邑。",
      "象曰": "升虚邑，无所疑也。",
      "时运": "越来越好，不必担心。",
      "财运": "货物集散，财利可期。",
      "家宅": "先虚后实；空房独守。",
      "身体": "虚弱之症。"
    },
    {
      "爻位": "六四",
      "爻辞": "王用亨于岐山，吉，无咎。",
      "象曰": "王用亨于岐山，顺事也。",
      "时运": "升迁在即，感谢神明。",
      "财运": "货物充裕，财神相助。",
      "家宅": "祭告宅神。",
      "身体": "虔诚祷告。"
    },
    {
      "爻位": "六五",
      "爻辞": "贞吉，升阶。",
      "象曰": "贞吉升阶，大得志也。",
      "时运": "大愿可成，功名皆吉。",
      "财运": "经营得中，利益不断。",
      "家宅": "步步高升；攀结高亲。",
      "身体": "病情升高。"
    },
    {
      "爻位": "上六",
      "爻辞": "冥升，利于不息之贞。",
      "象曰": "冥升在上，消不富也。",
      "时运": "好运已过，预备退路。",
      "财运": "经营困难，人财两失。",
      "家宅": "难免中落；难以偕老。",
      "身体": "可能归天。"
    }
  ]
}