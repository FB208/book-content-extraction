{
  "卦名": "旅卦",
  "卦辞": "小亨。旅贞吉。",
  "象曰": "山上有火，旅。君子以明慎用刑而不留狱。",
  "时运": "谨慎防災，升用在即。",
  "财运": "出外经营，不可积货。",
  "家宅": "小心火灾；即日成亲；",
  "身体": "肝火过旺，性命交关。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "旅琐琐，斯其所取灾。",
      "象曰": "旅琐琐，志穷灾也。",
      "时运": "所得有限，修行为宜。",
      "财运": "本小利微，小心灾祸。",
      "家宅": "谨慎免灾；小户联姻。",
      "身体": "病初即治。"
    },
    {
      "爻位": "六二",
      "爻辞": "旅即次，怀其资，得童仆，贞。",
      "象曰": "得童仆贞，终无尤也。",
      "时运": "运势中正，名利皆得。",
      "财运": "生财有道，作客无忧。",
      "家宅": "寄居亦福；富室赘婿。",
      "身体": "旅途有恙，受人照料。"
    },
    {
      "爻位": "九三",
      "爻辞": "旅焚其次，丧其童仆，贞厉。",
      "象曰": "旅焚其次，亦以伤矣。以旅与下，其义丧也。",
      "时运": "运势颠倒，危难不少。",
      "财运": "不必求利，早些防祸。",
      "家宅": "小心防火；难以偕老。",
      "身体": "孩子或童仆难保。"
    },
    {
      "爻位": "九四",
      "爻辞": "旅于处，得其资斧，我心不快。",
      "象曰": "旅于处，未得位也。得其资斧，心未快也。",
      "时运": "一时有困，来年再说。",
      "财运": "获利有限，心中不平。",
      "家宅": "地位不适；不是正室。",
      "身体": "忧郁不欢。"
    },
    {
      "爻位": "六五",
      "爻辞": "射雉，一矢亡，终以誉命。",
      "象曰": "终以誉命，上逮也。",
      "时运": "晚运甚佳，值得恭喜。",
      "财运": "小失大得，有利有名。",
      "家宅": "可称美善；佳偶天成。",
      "身体": "殉难受奖。"
    },
    {
      "爻位": "上九",
      "爻辞": "鸟焚其巢，旅人先笑后号咷。丧牛于易，凶。",
      "象曰": "以旅在上，其义焚也。丧牛于易，终莫之闻也。",
      "时运": "有失无得，乐极而悲。",
      "财运": "小利大损，十分凶险。",
      "家宅": "覆巢之险；先喜后悲。",
      "身体": "属牛者凶。"
    }
  ]
}