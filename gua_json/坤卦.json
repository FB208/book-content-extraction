{
  "卦名": "坤卦",
  "卦辞": "元亨，利牝马之贞。君子有攸往，先迷后得主。利西南得朋，东北丧朋。安贞吉。",
  "象曰": "地势坤，君子以厚德载物。",
  "时运": "为人厚道，声名远传。",
  "财运": "满载而归。",
  "家宅": "家庭安稳；婚嫁大吉。",
  "身体": "柔软运动。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "履霜，坚冰至。",
      "象曰": "履霜坚冰，阴始凝也。驯致其道，至坚冰也。",
      "时运": "由卑而尊，不可躁进。",
      "财运": "渐积可致富。",
      "家宅": "阴盛不吉；婚嫁不利。",
      "身体": "阴寒之症，久则难治。"
    },
    {
      "爻位": "六二",
      "爻辞": "直方大，不习，无不利。",
      "象曰": "六二之动，直以方也；不习，无不利，地道光也。",
      "时运": "功可成，名可就。",
      "财运": "获利可期。",
      "家宅": "居家如意；婚嫁顺利。",
      "身体": "不药而愈。"
    },
    {
      "爻位": "六三",
      "爻辞": "含章可贞。或从王事，无成有终。",
      "象曰": "含章可贞，以时发也；或从王事，知光大也。",
      "时运": "待时而发，可保功名。",
      "财运": "把握时机，必有利益。",
      "家宅": "凝聚向心力。",
      "身体": "无可挽回，凶。"
    },
    {
      "爻位": "六四",
      "爻辞": "括囊，无咎无誉。",
      "象曰": "括囊无咎，慎不害也。",
      "时运": "收敛为要，但求无过。",
      "财运": "落袋为安，不再投资。",
      "家宅": "平安是福。",
      "身体": "谨慎保养。"
    },
    {
      "爻位": "六五",
      "爻辞": "黄裳，元吉。",
      "象曰": "黄裳元吉，文在中也。",
      "时运": "功名大显。",
      "财运": "必定获利。",
      "家宅": "和乐融融。",
      "身体": "病在肠胃。"
    },
    {
      "爻位": "上六",
      "爻辞": "龙战于野，其血玄黄。",
      "象曰": "龙战于野，其道穷也。",
      "时运": "穷途末路。",
      "财运": "血本无归。",
      "家宅": "失序不和。",
      "身体": "肝血失调，危险。"
    }
  ]
}