{
  "卦名": "同人卦",
  "卦辞": "同人。同人于野，亨。利涉大川，利君子贞。",
  "象曰": "天与火，同人。君子以类族辨物。",
  "时运": "朋友支持，升迁顺利。",
  "财运": "合资有利，可以进取。",
  "家宅": "阖家欢喜。",
  "身体": "燥热之症，另求良医。",
  "动爻": [
    {
      "爻位": "初九",
      "爻辞": "同人于门，无咎。",
      "象曰": "出门同人，又谁咎也？",
      "时运": "眼前平顺，外出经营。",
      "财运": "不宜开店，可以行商。",
      "家宅": "家人和睦。",
      "身体": "避地调养，可以无碍。"
    },
    {
      "爻位": "六二",
      "爻辞": "同人于宗，吝。",
      "象曰": "同人于宗，吝道也。",
      "时运": "相忌者多，未能顺遂。",
      "财运": "大宗买卖，留心出纳。",
      "家宅": "长子之力，勤俭起家。",
      "身体": "魂归宗庙，未可多言。"
    },
    {
      "爻位": "九三",
      "爻辞": "伏戎于莽，升其高陵，三岁不兴。",
      "象曰": "伏戎于莽，敌刚也，三岁不兴，安行也。",
      "时运": "潜心三年，再谋其事。",
      "财运": "可开山林，三年获利。",
      "家宅": "须防盗贼。",
      "身体": "不良于行。"
    },
    {
      "爻位": "九四",
      "爻辞": "乘其墉，弗克攻，吉。",
      "象曰": "乘其墉，义弗克也；其吉，则困而反则也。",
      "时运": "退守不动，反而吉祥。",
      "财运": "守货不售，将可获利。",
      "家宅": "修缮房屋围墙。",
      "身体": "虽有凶险，终究无害。"
    },
    {
      "爻位": "九五",
      "爻辞": "同人，先号啕而后笑，大师克相遇。",
      "象曰": "同人之先，以中直也。大师相遇，言相克也。",
      "时运": "辛苦有成，得偿心愿。",
      "财运": "小有挫折，终有大利。",
      "家宅": "不必惊慌，终于平安。",
      "身体": "先危后安。"
    },
    {
      "爻位": "上九",
      "爻辞": "同人于郊，无悔。",
      "象曰": "同人于郊，志未得也。",
      "时运": "闲散之地，诸事无碍。",
      "财运": "郊外立业，暂时无利。",
      "家宅": "平顺无灾。",
      "身体": "恐无生机。"
    }
  ]
}