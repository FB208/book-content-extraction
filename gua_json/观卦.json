{
  "卦名": "观卦",
  "卦辞": "亨。盥而不荐，有孚颙若。",
  "象曰": "风行地上，观。君子以省方观民设教。",
  "时运": "出外游览，不易闲居。",
  "财运": "贩卖洋货，须防风险。",
  "家宅": "供养神佛。",
  "身体": "风湿之症，运动调养。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "童观，小人无咎，君子吝。",
      "象曰": "初六童观，小人道也。",
      "时运": "初运未佳，但无大碍。",
      "财运": "初登场面，小作即可。",
      "家宅": "小心童仆；自由结亲。",
      "身体": "小孩没事，大人不利。"
    },
    {
      "爻位": "六二",
      "爻辞": "窥观，利女贞。",
      "象曰": "窥观女贞，亦可丑也。",
      "时运": "最好退守，女性有利。",
      "财运": "蚕丝有利，余皆不宜。",
      "家宅": "妇女主家。",
      "身体": "阴寒之症，可愈。"
    },
    {
      "爻位": "六三",
      "爻辞": "观我生，进退。",
      "象曰": "观我生进退，未失道也。",
      "时运": "度德量力，稍安勿躁。",
      "财运": "随买随卖，不致有失。",
      "家宅": "守住旧债。",
      "身体": "安心静养，可保平安。"
    },
    {
      "爻位": "九四",
      "爻辞": "观国之光，利用宾于王。",
      "象曰": "观国之光，尚宾也。",
      "时运": "得人赏识，功名可期。",
      "财运": "货物流通，利润自来。",
      "家宅": "一家和睦；外地订亲，吉。",
      "身体": "多做户外运动。"
    },
    {
      "爻位": "九五",
      "爻辞": "观我生，君子无咎。",
      "象曰": "观我生，观民也。",
      "时运": "直道而行，无往不利。",
      "财运": "公平经营，取得正财。",
      "家宅": "与富贵人结婚。",
      "身体": "吐纳之术保健康。"
    },
    {
      "爻位": "上九",
      "爻辞": "观其生，君子无咎。",
      "象曰": "观其生，志未平也。",
      "时运": "闲散之地，诸事无碍。",
      "财运": "郊外立业，暂时无利。",
      "家宅": "平顺无灾。",
      "身体": "恐无生机。"
    }
  ]
}