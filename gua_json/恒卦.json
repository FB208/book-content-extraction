{
  "卦名": "恒卦",
  "卦辞": "亨，无咎，利贞。",
  "象曰": "雷风，恒。君子以立不易方。",
  "时运": "努力耕耘，不可躁进。",
  "财运": "贸易之地，不可更改。",
  "家宅": "方向不改；百年好合。",
  "身体": "气喘有痰，仍服旧方。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "浚恒，贞凶，无攸利。",
      "象曰": "浚恒之凶，始求深也。",
      "时运": "安分知足，求荣反辱。",
      "财运": "得利即售，勿贪高价。",
      "家宅": "华丽难久；不宜攀结。",
      "身体": "运动伤害，操之过急。"
    },
    {
      "爻位": "九二",
      "爻辞": "悔亡。",
      "象曰": "九二悔亡，能久中也。",
      "时运": "稳住阵脚，可以免祸。",
      "财运": "苦撑待变，将可回本。",
      "家宅": "位置不利，须待十年。",
      "身体": "尚称平顺。"
    },
    {
      "爻位": "九三",
      "爻辞": "不恒其德，或承之羞，贞吝。",
      "象曰": "不恒其德，无所容也。",
      "时运": "三心二意，如何成功。",
      "财运": "没有恒业，难以获利。",
      "家宅": "不利久居；难以偕老。",
      "身体": "运动无恒，如何健康。"
    },
    {
      "爻位": "九四",
      "爻辞": "田无禽。",
      "象曰": "久非其位，安得禽也？",
      "时运": "不得正位，徒劳无功。",
      "财运": "地方不对，如何有利。",
      "家宅": "方位不利；配偶不和。",
      "身体": "服药谨慎。"
    },
    {
      "爻位": "六五",
      "爻辞": "恒其德，贞。妇人吉，夫子凶。",
      "象曰": "妇人贞吉，从一而终也。夫子制义，从妇凶也。",
      "时运": "迷恋感情，因小失大。",
      "财运": "只见小利，如何致富。",
      "家宅": "妻强夫弱；女占喜，男占凶。",
      "身体": "男女异命。"
    },
    {
      "爻位": "上六",
      "爻辞": "振恒，凶。",
      "象曰": "振恒在上，大无功也。",
      "时运": "功名已尽，不可妄动。",
      "财运": "不愿结算，无利可言。",
      "家宅": "旧宅不改；再娶必凶。",
      "身体": "小心保养。"
    }
  ]
}