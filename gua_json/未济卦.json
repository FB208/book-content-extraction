{
  "卦名": "未济卦",
  "卦辞": "亨。小狐汔济，濡其尾，无攸利。",
  "象曰": "火在水上，未济。君子以慎辨物居方。",
  "时运": "运势颠倒，务必谨慎。",
  "财运": "诸事不顺，步步为营。",
  "家宅": "改变方向；门户不合。",
  "身体": "血脉不顺，用药谨慎。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "濡其尾，吝。",
      "象曰": "濡其尾，亦不知极也。",
      "时运": "鲁莽行事，无路可走。",
      "财运": "不记教训，谋财无方。",
      "家宅": "门户方位不正；团聚不易。",
      "身体": "病在下身。"
    },
    {
      "爻位": "九二",
      "爻辞": "曳其轮，贞吉。",
      "象曰": "九二贞吉，中以行正也。",
      "时运": "上下相得，无往不利。",
      "财运": "用心经营，满载而归。",
      "家宅": "美善之家；必得贤妇。",
      "身体": "胸腹阻滞。"
    },
    {
      "爻位": "六三",
      "爻辞": "未济，征凶，利涉大川。",
      "象曰": "未济征凶，位不当也。",
      "时运": "见机而作，妄动则凶。",
      "财运": "把握良机，船运有利。",
      "家宅": "安居为宜；迎娶为吉。",
      "身体": "预防水祟，渡川以解。"
    },
    {
      "爻位": "九四",
      "爻辞": "贞吉，悔亡，震用伐鬼方。三年有赏于大国。",
      "象曰": "贞吉悔亡，志行也。",
      "时运": "志得意满，名利双全。",
      "财运": "远方经营，利润可观。",
      "家宅": "安居乐业；二年成婚。",
      "身体": "可保三年。"
    },
    {
      "爻位": "六五",
      "爻辞": "贞吉，无悔，君子之光。有孚，吉。",
      "象曰": "君子之光，其晖吉也。",
      "时运": "运势正旺，诸事皆吉。",
      "财运": "官方业务，获利稳定。",
      "家宅": "大吉之家；婚姻必成。",
      "身体": "神清气爽。"
    },
    {
      "爻位": "上九",
      "爻辞": "有孚于饮酒，无咎。濡其首，有孚失是。",
      "象曰": "饮酒濡首，亦不知节也。",
      "时运": "能知节俭，可保无虞。",
      "财运": "量入为出，永享其富。",
      "家宅": "勤俭持家；相敬如宾。",
      "身体": "饮食失节，不可不慎。"
    }
  ]
}