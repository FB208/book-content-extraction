{
  "卦名": "比卦",
  "卦辞": "吉。原筮，元永贞，无咎。不宁方来，后夫凶。",
  "象曰": "地上有水，比。先王以建万国，亲诸侯。",
  "时运": "众人相贺，荣显之极。",
  "财运": "善人相扶，大发利市。",
  "家宅": "家庭安稳；婚嫁大吉。",
  "身体": "柔软运动。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "有孚，比之，无咎。有孚盈缶，终来有它吉。",
      "象曰": "比之初六，有它吉也。",
      "时运": "诚信交往，声名日隆。",
      "财运": "信用卓著，利益自来。",
      "家宅": "社区和睦；亲事亦谐。",
      "身体": "平安无事。"
    },
    {
      "爻位": "六二",
      "爻辞": "比之自内，贞吉。",
      "象曰": "比之自内，不自失也。",
      "时运": "实至名归，自然吉祥。",
      "财运": "同心协力，获利可期。",
      "家宅": "亲上加亲。",
      "身体": "心平气和，疾病可愈。"
    },
    {
      "爻位": "六三",
      "爻辞": "比之匪人。",
      "象曰": "比之匪人，不亦伤乎？",
      "时运": "交友不正，声名破败。",
      "财运": "所托非人，损耗难免。",
      "家宅": "戒慎嫁娶，以免遗憾。",
      "身体": "改求良医。"
    },
    {
      "爻位": "六四",
      "爻辞": "外比之，贞吉。",
      "象曰": "外比于贤，以从上也。",
      "时运": "得人赏识，功名可期。",
      "财运": "货物流通，利润自来。",
      "家宅": "一家和睦；外地订亲，吉。",
      "身体": "多做户外运动。"
    },
    {
      "爻位": "九五",
      "爻辞": "显比，王用三驱，失前禽。邑人不诫，吉。",
      "象曰": "显比之吉，位正中也。舍逆取顺，失前禽也。邑人不诫，上使中也。",
      "时运": "善待别人，后仍有吉。",
      "财运": "不贪小利，后有盈余。",
      "家宅": "和顺为宜。",
      "身体": "服药生效，不必过虑。"
    },
    {
      "爻位": "上六",
      "爻辞": "比之无首，凶。",
      "象曰": "比之无首，无所终也。",
      "时运": "六神无主，恐有凶祸。",
      "财运": "白费心机，一无所获。",
      "家宅": "恐丧家主；婚嫁来历不明。",
      "身体": "小心头部疾病。"
    }
  ]
}