{
  "卦名": "艮卦",
  "卦辞": "艮其背，不获其身。行其庭，不见其人。无咎。",
  "象曰": "兼山，艮。君子以思不出其位。",
  "时运": "运势平平，不宜妄进。",
  "财运": "守好本业，不可贪财。",
  "家宅": "不宜改造；命由前定。",
  "身体": "带病延年。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "艮其趾，无咎。利永贞。",
      "象曰": "艮其趾，未失正也。",
      "时运": "初交好运，退守无咎。",
      "财运": "知足常乐，多行善事。",
      "家宅": "可以长住；百年好合。",
      "身体": "足疾就医。"
    },
    {
      "爻位": "六二",
      "爻辞": "艮其腓，不拯其随。其心不快。",
      "象曰": "不拯其随，未退听也。",
      "时运": "运途受阻，缺少援手。",
      "财运": "止而不售，难免心忧。",
      "家宅": "不宜迁居；避开此婚。",
      "身体": "药石难治。"
    },
    {
      "爻位": "九三",
      "爻辞": "艮其限，列其夤，厉薰心。",
      "象曰": "艮其限，危薰心也。",
      "时运": "顺时可成，不可勉强。",
      "财运": "闭关自守，难免穷困。",
      "家宅": "往来为宜；不拘门户。",
      "身体": "血脉不通。"
    },
    {
      "爻位": "六四",
      "爻辞": "艮其身，无咎。",
      "象曰": "艮其身，止诸躬也。",
      "时运": "无得无失，保身无咎。",
      "财运": "可以保本，另得良机。",
      "家宅": "平安无事；婚姻平平。",
      "身体": "带病延年。"
    },
    {
      "爻位": "六五",
      "爻辞": "艮其辅，言有序，悔亡。",
      "象曰": "艮其辅，以中正也。",
      "时运": "言谈中正，自然可取。",
      "财运": "保密为要，商机可成。",
      "家宅": "位得中正；慎防巧言。",
      "身体": "口能发声，病即可治。"
    },
    {
      "爻位": "上九",
      "爻辞": "敦艮，吉。",
      "象曰": "敦艮之吉，以厚终也。",
      "时运": "好上加好，自然吉祥。",
      "财运": "上手生意，获利自多。",
      "家宅": "世代忠厚；婚姻吉祥。",
      "身体": "体质厚实。"
    }
  ]
}