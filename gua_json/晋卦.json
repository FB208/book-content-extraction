{
  "卦名": "晋卦",
  "卦辞": "元亨，利贞",
  "象曰": "明出地上，晋。君子以自昭明德",
  "时运": "好运新来，步步高升",
  "财运": "光亮之业，最有利润",
  "家宅": "阳光之屋",
  "身体": "虚火上升，小心诊治",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "晋如，摧如，贞吉。罔孚，裕，无咎",
      "象曰": "晋如摧如，独行正也。裕无咎，未受命也",
      "时运": "耐心等待，不必急进",
      "财运": "稍待时日，可获大利",
      "家宅": "吉屋可居；婚姻缓成",
      "身体": "宽心解怀"
    },
    {
      "爻位": "六二",
      "爻辞": "晋如，愁如，贞吉。受兹介福，于其王母",
      "象曰": "受兹介福，以中正也",
      "时运": "所求多阻，守正必亨",
      "财运": "守住低潮，自然受福",
      "家宅": "迁居与老人同住；婚姻稍待",
      "身体": "多听老人言"
    },
    {
      "爻位": "六三",
      "爻辞": "众允，悔亡",
      "象曰": "众允之志，上行也",
      "时运": "众人悦服，自无懊恼",
      "财运": "双方和睦，买卖皆利",
      "家宅": "气氛和谐；两性融洽",
      "身体": "团体运动；讼事调解"
    },
    {
      "爻位": "九四",
      "爻辞": "晋如鼫鼠，贞厉",
      "象曰": "鼫鼠贞厉，位不当也",
      "时运": "守正为宜，耍诈必凶",
      "财运": "贪财必败，见好就收",
      "家宅": "耗失过多；婚姻不正",
      "身体": "疥疮或呕血，皆危"
    },
    {
      "爻位": "六五",
      "爻辞": "悔亡，失得勿恤，往吉，无不利",
      "象曰": "失得勿恤，往有庆也",
      "时运": "灾去福来，无意得之",
      "财运": "前有小失，今可大得",
      "家宅": "屋运转好；婚姻吉祥",
      "身体": "已无大碍"
    },
    {
      "爻位": "上九",
      "爻辞": "晋其角，维用伐邑。厉吉无咎，贞吝",
      "象曰": "维用伐邑，道未光也",
      "时运": "好运将终，防有事故",
      "财运": "同业纷争，幸可无咎",
      "家宅": "邻里不安；始争终和",
      "身体": "保养头部；罢讼为吉"
    }
  ]
}