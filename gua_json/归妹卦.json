{
  "卦名": "归妹卦",
  "卦辞": "征凶，无攸利",
  "象曰": "泽上有雷，归妹。君子以永终知敝",
  "时运": "进不以道，难以持久",
  "财运": "货价尚可，结局未必",
  "家宅": "已婚不宜居母家；勿耽情欲",
  "身体": "大限将至",
  "动爻": [
    {
      "爻位": "初九",
      "爻辞": "归妹以娣，跛能履，征吉",
      "象曰": "归妹以娣，以恒也。跛能履，吉相承也",
      "时运": "因人成事，乏善可陈",
      "财运": "奉命而行，幸可获利",
      "家宅": "偏屋亦吉",
      "身体": "不良于行"
    },
    {
      "爻位": "九二",
      "爻辞": "眇能视，利幽人之贞",
      "象曰": "利幽人之贞，未变常也",
      "时运": "不必幻想，安心修养",
      "财运": "独到之见，暗中得利",
      "家宅": "适合隐居；偏房为宜",
      "身体": "保养眼睛"
    },
    {
      "爻位": "六三",
      "爻辞": "归妹以须，反归以娣",
      "象曰": "归妹以须，未当也",
      "时运": "受人抑制，忍耐一时",
      "财运": "眼前无利，可图未来",
      "家宅": "不是正宅；不是正娶",
      "身体": "待时可愈"
    },
    {
      "爻位": "九四",
      "爻辞": "归妹愆期，迟归有时",
      "象曰": "愆期之志，有待而行也",
      "时运": "行运有时，不宜躁进",
      "财运": "待时而动，自可获利",
      "家宅": "暂勿迁居；须待时日",
      "身体": "安心静养"
    },
    {
      "爻位": "六五",
      "爻辞": "帝乙归妹，其君之袂不如其娣之袂良。月几望，吉",
      "象曰": "帝乙归妹，不如其娣之袂良也。其位在中，以贵行也",
      "时运": "谦虚得福，连袂上进",
      "财运": "后货较优，早售有利",
      "家宅": "喜事临门；二女同归",
      "身体": "半月可愈"
    },
    {
      "爻位": "上六",
      "爻辞": "女承筐无实，士刲羊无血，无攸利",
      "象曰": "上六无实，承虚筐也",
      "时运": "诸事不顺，小心为宜",
      "财运": "本虚货缺，如何得利",
      "家宅": "须防纷争；婚娶不正",
      "身体": "虚劳失血，不治之象"
    }
  ]
}