{
  "卦名": "师卦",
  "卦辞": "贞，丈人吉，无咎。",
  "象曰": "地中有水，师。君子以容民畜众。",
  "时运": "包容别人，修行待时。",
  "财运": "有财有库，善自珍惜。",
  "家宅": "旧亲联姻，可喜可贺。",
  "身体": "腹胀之症，调气无忧。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "师出以律，否臧，凶。",
      "象曰": "师出以律，失律凶也。",
      "时运": "失道以求，终受其辱。",
      "财运": "海运业务，合义则吉。",
      "家宅": "治家以法，否则必凶。",
      "身体": "寒多热少，宜早医治。"
    },
    {
      "爻位": "九二",
      "爻辞": "在师中，吉无咎，王三锡命。",
      "象曰": "在师中吉，承天宠也。王三锡命，怀万邦也。",
      "时运": "杰出人才，大受賞识。",
      "财运": "谋略出众，领导获利。",
      "家宅": "邻里所重；婚姻吉祥。",
      "身体": "流动血气，病即舒解。"
    },
    {
      "爻位": "六三",
      "爻辞": "师或舆尸，凶。",
      "象曰": "师或舆尸，大无功也。",
      "时运": "无德有位，难免于凶。",
      "财运": "物耗财损，凶险无比。",
      "家宅": "不得安宁，小心为上。",
      "身体": "可能归天。"
    },
    {
      "爻位": "六四",
      "爻辞": "师左次，无咎。",
      "象曰": "左次无咎，未失常也。",
      "时运": "官运不济，不宜妄想。",
      "财运": "次级货物，尚可获利。",
      "家宅": "近东，朝西南吉；入赘无咎。",
      "身体": "春生气来，疾病可愈。"
    },
    {
      "爻位": "六五",
      "爻辞": "田有禽，利执言，无咎。长子帅师，弟子舆尸，贞凶。",
      "象曰": "长子帅师，以中行也；弟子舆尸，使不当也。",
      "时运": "德胜于才，否则凶险。",
      "财运": "老成练达，有利可图。",
      "家宅": "利于长子；媒妁而成。",
      "身体": "跑步健身。"
    },
    {
      "爻位": "上六",
      "爻辞": "大君有命，开国承家，小人勿用。",
      "象曰": "大君有命，以正功也；小人勿用，必乱邦也。",
      "时运": "论功行赏，勿做小人。",
      "财运": "因富致贵，须防小人。",
      "家宅": "家道兴旺；贵人做媒。",
      "身体": "正常运动。"
