{
  "卦名": "震卦",
  "卦辞": "亨。震来虩虩，笑言哑哑。震惊百里，不丧匕鬯。",
  "象曰": "雷在天上，震。君子以恐惧修省。",
  "时运": "运势正强，谨慎免咎。",
  "财运": "所积财物，皆得售出。",
  "家宅": "保护宅基，祭祷为宜；佳偶。",
  "身体": "肝火太盛，不宜劳累。",
  "动爻": [
    {
      "爻位": "初九",
      "爻辞": "震来虩虩，后笑言哑哑，吉。",
      "象曰": "震来虩虩，恐致福也。笑言哑哑，后有则也。",
      "时运": "好运新来，先苦后乐。",
      "财运": "用心经营，得利可乐。",
      "家宅": "谨慎为宜；先忧后喜。",
      "身体": "先危后安。"
    },
    {
      "爻位": "六二",
      "爻辞": "震来厉，亿丧贝。跻于九陵，勿逐，七日得。",
      "象曰": "震来厉，乘刚也。",
      "时运": "患得患失，不必过虑。",
      "财运": "有得有失，危机意识。",
      "家宅": "注意防盗；夫妻不睦。",
      "身体": "病势可忧，七日可愈。"
    },
    {
      "爻位": "六三",
      "爻辞": "震苏苏，震行无眚。",
      "象曰": "震苏苏，位不当也。",
      "时运": "加倍谨慎，可以免咎。",
      "财运": "销路不佳，另行设法。",
      "家宅": "小心地震；门户不当。",
      "身体": "由危而安。"
    },
    {
      "爻位": "九四",
      "爻辞": "震遂泥。",
      "象曰": "震遂泥，未光也。",
      "时运": "欲振乏力，退而自保。",
      "财运": "挥金如土，难有积蓄。",
      "家宅": "阳气阻塞；辛苦成家。",
      "身体": "腹部积滞，宜泄除之。"
    },
    {
      "爻位": "六五",
      "爻辞": "震往来厉，亿无丧，有事。",
      "象曰": "震往来厉，危行也。其事在中，大无丧也。",
      "时运": "患难已过，可成大事。",
      "财运": "小损大利，不必多忧。",
      "家宅": "祭祷防祟。",
      "身体": "修身养性。"
    },
    {
      "爻位": "上六",
      "爻辞": "震索索，视矍矍，征凶。震不于其躬，于其邻，无咎。婚媾有言。",
      "象曰": "震索索，中未得也。虽凶无咎，畏邻戒也。",
      "时运": "位高必危，静守为宜。",
      "财运": "须防过贪，谨守小成。",
      "家宅": "邻居有事；近时媒来。",
      "身体": "安心养目。"
    }
  ]
}