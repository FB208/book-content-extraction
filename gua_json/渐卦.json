{
  "卦名": "渐卦",
  "卦辞": "亨，利贞。",
  "象曰": "山上有木，渐。君子以居贤德善俗。",
  "时运": "时来运转，可以得意。",
  "财运": "逐渐得利，多行善事。",
  "家宅": "君子居之；贤女可妻。",
  "身体": "安居调养。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "鸿渐于干，小子厉。有言，无咎。",
      "象曰": "小子之厉，义无咎也。",
      "时运": "初行好运，要有耐心。",
      "财运": "有约在先，宜防小人。",
      "家宅": "尚无大碍；女长男少。",
      "身体": "大人没事，小孩就医。"
    },
    {
      "爻位": "六二",
      "爻辞": "鸿渐于磐，饮食衎衎，吉。",
      "象曰": "饮食衎衎，不素饱也。",
      "时运": "嘉宾安乐，名利皆有。",
      "财运": "日益增加，稳若磐石。",
      "家宅": "和乐相处；百年偕老。",
      "身体": "饮食过度之症。"
    },
    {
      "爻位": "九三",
      "爻辞": "鸿渐于陆，夫征不复，妇孕不育，凶。利用御寇。",
      "象曰": "夫征不复，离群丑也。妇孕不育，失其道也。利用御寇，顺相保也。",
      "时运": "运势不正，须防有祸。",
      "财运": "不易得利，防有盗贼。",
      "家宅": "不利生产，须防离散。",
      "身体": "生产时恐难两全。"
    },
    {
      "爻位": "六四",
      "爻辞": "鸿渐于木，或得其桷，无咎。",
      "象曰": "或得其桷，顺以巽也。",
      "时运": "随遇而安，可以免咎。",
      "财运": "利润甚微，保本即可。",
      "家宅": "可能寡居。",
      "身体": "肝火过盛。"
    },
    {
      "爻位": "九五",
      "爻辞": "鸿渐于陵，妇三岁不孕，终莫之胜，吉。",
      "象曰": "终莫之胜，吉，得所愿也。",
      "时运": "运势中正，三年必成。",
      "财运": "眼前平平，三年大发。",
      "家宅": "可以安居；得子稍迟。",
      "身体": "三年可愈。"
    },
    {
      "爻位": "上九",
      "爻辞": "鸿渐于陆，其羽可用为仪，吉。",
      "象曰": "其羽可用为仪吉，不可乱也。",
      "时运": "大运来到，可以用世。",
      "财运": "货美价高，自然获利。",
      "家宅": "辉煌可观；婚姻吉祥。",
      "身体": "健康活泼。"
    }
  ]
}