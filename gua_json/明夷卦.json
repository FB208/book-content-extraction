{
  "卦名": "明夷卦",
  "卦辞": "利艰贞",
  "象曰": "明入地中，明夷。君子以莅众用晦而明",
  "时运": "明哲保身，以避灾厄",
  "财运": "明无利润，暗中分红",
  "家宅": "父子分居为宜；不是明媒正娶",
  "身体": "肝大郁积，注意保养",
  "动爻": [
    {
      "爻位": "初九",
      "爻辞": "明夷于飞，垂其翼。君子于行，三日不食。有攸往，主人有言",
      "象曰": "君子于行，义不食也",
      "时运": "培养实力，等待机会",
      "财运": "宜守不宜攻",
      "家宅": "娶妻小心",
      "身体": "保养为宜"
    },
    {
      "爻位": "六二",
      "爻辞": "明夷，夷于左股，用拯马壮，吉",
      "象曰": "六二之吉，顺以则也",
      "时运": "培养实力，等待机会",
      "财运": "宜守不宜攻",
      "家宅": "娶妻小心",
      "身体": "保养为宜"
    },
    {
      "爻位": "九三",
      "爻辞": "明夷于南狩，得其大首，不可疾，贞",
      "象曰": "南狩之志，乃大得也",
      "时运": "培养实力，等待机会",
      "财运": "宜守不宜攻",
      "家宅": "娶妻小心",
      "身体": "保养为宜"
    },
    {
      "爻位": "六四",
      "爻辞": "入于左腹，获明夷之心，于出门庭",
      "象曰": "入于左腹，获心意也",
      "时运": "培养实力，等待机会",
      "财运": "宜守不宜攻",
      "家宅": "娶妻小心",
      "身体": "保养为宜"
    },
    {
      "爻位": "六五",
      "爻辞": "箕子之明夷，利贞",
