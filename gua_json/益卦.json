{
  "卦名": "益卦",
  "卦辞": "元亨，利贞。",
  "象曰": "风雷，益。君子以见善则迁，有过则改。",
  "时运": "得意之时，改旧换新。",
  "财运": "贸易要快，才有利益。",
  "家宅": "小心风雷；婚姻好合。",
  "身体": "肝火太盛。",
  "动爻": [
    {
      "爻位": "初九",
      "爻辞": "利用为大作，元吉，无咎。",
      "象曰": "元吉无咎，下不厚事也。",
      "时运": "大事可为，无不如意。",
      "财运": "有人有谋，大利在前。",
      "家宅": "新宅宽大；婚姻大吉。",
      "身体": "健康无虞。"
    },
    {
      "爻位": "六二",
      "爻辞": "或益之十朋之龟，弗克违。永贞吉，王用享于帝，吉。",
      "象曰": "或益之，自外来也。",
      "时运": "意外得财，又能守住。",
      "财运": "如有神助，必得厚利。",
      "家宅": "安居之家；百年好合。",
      "身体": "祈祷可愈。"
    },
    {
      "爻位": "六三",
      "爻辞": "益之用凶事，无咎。有孚中行，告公用圭。",
      "象曰": "益用凶事，固有之也。",
      "时运": "先苦后甘，讲信修睦。",
      "财运": "欲求富贵，必须冒险。",
      "家宅": "逢凶化吉；苦中成婚。",
      "身体": "有惊无险。"
    },
    {
      "爻位": "六四",
      "爻辞": "中行，告公从，利用为依迁国。",
      "象曰": "告公从，以益志也。",
      "时运": "眼前有难，暂避为宜。",
      "财运": "改迁他处，另开店面。",
      "家宅": "最好迁移；另找媒妁。",
      "身体": "外地就医。"
    },
    {
      "爻位": "九五",
      "爻辞": "有孚惠心，勿问元吉。有孚惠我德。",
      "象曰": "有孚惠心，勿问之矣。惠我德，大得志也。",
      "时运": "存心仁厚，实至名归。",
      "财运": "兼顾道义，利益长久。",
      "家宅": "善人之居；非亲即友。",
      "身体": "保养得宜。"
    },
    {
      "爻位": "上九",
      "爻辞": "莫益之，或击之。立心勿恒，凶。",
      "象曰": "莫益之，偏辞也；或击之，自外来也。",
      "时运": "贪求名位，意外之祸。",
      "财运": "专求己利，必生争端。",
      "家宅": "不可久居；不易偕老。",
      "身体": "无恒之凶。"
    }
  ]
}