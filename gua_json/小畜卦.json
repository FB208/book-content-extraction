{
  "卦名": "小畜卦",
  "卦辞": "亨。密云不雨，自我西郊。",
  "象曰": "风行天上，小畜。君子以懿文德。",
  "时运": "平平无奇，受人牵制。",
  "财运": "外表不错，内多耗损。",
  "家宅": "小康，须防口舌；娶得淑女。",
  "身体": "风火之症；小孩吉，大人凶。",
  "动爻": [
    {
      "爻位": "初九",
      "爻辞": "复自道，何其咎？吉。",
      "象曰": "复自道，其义吉也。",
      "时运": "退守自保，无灾无难。",
      "财运": "守稳旧业，不宜创新。",
      "家宅": "不求于外，家道自亨。",
      "身体": "安静休养，可以复原。"
    },
    {
      "爻位": "九二",
      "爻辞": "牵复，吉。",
      "象曰": "牵复在中，亦不自失也。",
      "时运": "因人成事，获利大吉。",
      "财运": "创业守成，两皆为宜。",
      "家宅": "兄弟和睦，家道兴隆。",
      "身体": "旧疾复发，小心调养。"
    },
    {
      "爻位": "九三",
      "爻辞": "舆说辐，夫妻反目。",
      "象曰": "夫妻反目，不能正室也。",
      "时运": "阴盛阳衰，内外不安。",
      "财运": "逆向操作，可以获利。",
      "家宅": "家庭不和，婚姻不利。",
      "身体": "阴阳不察，慎择良医。"
    },
    {
      "爻位": "六四",
      "爻辞": "有孚，血去惕出，无咎。",
      "象曰": "有孚惕出，上合志也。",
      "时运": "切忌争斗，出门远避；升迁靠长官提拔。",
      "财运": "利西北，不利东南；邻里相助。",
      "家宅": "管理得宜，否则受累。",
      "身体": "宽解治之，燥烈之药不宜。"
    },
    {
      "爻位": "九五",
      "爻辞": "有孚挛如，富以其邻。",
      "象曰": "有孚挛如，不独富也。",
      "时运": "一时亨通，无往不利。",
      "财运": "百货聚积，自有大利。",
      "家宅": "既富且贵，惠及邻里。",
      "身体": "手足麻痹之症。"
    },
    {
      "爻位": "上九",
      "爻辞": "既雨既处，尚德载。妇贞厉。月既望，君子征凶。",
      "象曰": "既雨既处，德积载也。君子征凶，有所疑也。",
      "时运": "长期梦想，终可如愿。",
      "财运": "得利则止，切忌过贪。",
      "家宅": "前困后亨。",
      "身体": "须防营养过多。"
    }
  ]
}