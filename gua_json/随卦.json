{
  "卦名": "随卦",
  "卦辞": "元亨，利贞，无咎",
  "象曰": "泽中有雷，随。君子以向晦入宴息",
  "时运": "明年远行，五年方展",
  "财运": "堆积存货，明春有利",
  "家宅": "防止惊惧",
  "身体": "休养为宜；牢狱之灾",
  "动爻": [
    {
      "爻位": "初九",
      "爻辞": "官有渝，贞吉。出门交有功",
      "象曰": "官有渝，从正吉也；出门交有功，不失也",
      "时运": "交运之时，利于变动",
      "财运": "货物外售，可以得利",
      "家宅": "装修迁居",
      "身体": "改变用药，远方求医"
    },
    {
      "爻位": "六二",
      "爻辞": "系小子，失丈夫",
      "象曰": "系小子，弗兼与也",
      "时运": "形势颠倒，最好谨慎",
      "财运": "贪小失大，显然不利",
      "家宅": "家主受累；婚嫁不妥",
      "身体": "顾此失彼"
    },
    {
      "爻位": "六三",
      "爻辞": "系丈夫，失小子。随有求，得，利居贞",
      "象曰": "系丈夫，志舍下也",
      "时运": "求财求名，皆为正运",
      "财运": "小往大来，利益可期",
      "家宅": "积蓄有成，关心子女",
      "身体": "大人无妨，小孩不利"
    },
    {
      "爻位": "九四",
      "爻辞": "随有获，贞凶。有孚，在道以明，何咎",
      "象曰": "随有获，其义凶也。有孚在道，明功也",
      "时运": "吉凶互见，明年吉祥",
      "财运": "虽有获利，小心意外",
      "家宅": "置屋不宜",
      "身体": "先凶后吉"
    },
    {
      "爻位": "九五",
      "爻辞": "孚于嘉，吉",
      "象曰": "孚于嘉吉，位正中也",
      "时运": "正当好运，诸事皆吉",
      "财运": "货物上品，得利不少",
      "家宅": "积善之家；婚嫁可喜",
      "身体": "毋须担心"
    },
    {
      "爻位": "上六",
      "爻辞": "拘系之，乃从维之；王用亨于西山",
      "象曰": "拘系之，上穷也",
      "时运": "困难重重，难以如意",
      "财运": "稳扎稳打，未能伸展",
      "家宅": "家人抱怨；嫁娶有缘",
      "身体": "诚心祷告；可能归魂"
    }
  ]
}