{
  "卦名": "豫卦",
  "卦辞": "利建侯行师",
  "象曰": "雷出地奋，豫。先王以作乐崇德，殷荐之上帝，以配祖考",
  "时运": "春雷发动，诸事吉祥",
  "财运": "新货上市，必有大利",
  "家宅": "祈神祭祖，可保平安",
  "身体": "祷告静心",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "鸣豫，凶",
      "象曰": "初六鸣豫，志穷凶也",
      "时运": "得意忘形，致遭困境",
      "财运": "初可得利，切忌过贪",
      "家宅": "怪异之惊，应防凶险",
      "身体": "颇为不利"
    },
    {
      "爻位": "六二",
      "爻辞": "介于石，不终日，贞吉",
      "象曰": "不终日，贞吉，以中正也",
      "时运": "人品高尚，不逐浮华",
      "财运": "自定其志，快速获利",
      "家宅": "严正持家，拒斥小人",
      "身体": "新病可愈，宿疾即亡"
    },
    {
      "爻位": "六三",
      "爻辞": "盱豫，悔。迟有悔",
      "象曰": "盱豫有悔，位不当也",
      "时运": "本身不正，因而有悔",
      "财运": "恐有损失，不如舍去",
      "家宅": "不循正途求婚嫁，将有困难",
      "身体": "不可纵欲伤身"
    },
    {
      "爻位": "九四",
      "爻辞": "由豫，大有得。勿疑，朋盍簪",
      "象曰": "求而往，明也",
      "时运": "顺势而为，大有发展",
      "财运": "明见利益，应有收获",
      "家宅": "婚嫁吉祥",
      "身体": "遵医生嘱咐"
    },
    {
      "爻位": "六五",
      "爻辞": "屯其膏，小贞吉，大贞凶",
      "象曰": "屯其膏，施未光也",
      "时运": "无人提拔，暂时无望",
      "财运": "小买卖尚可获利，大买卖则凶",
      "家宅": "聚财伤和气",
      "身体": "初病可治，久病则危"
    },
    {
      "爻位": "上六",
      "爻辞": "乘马班如，泣血涟如",
      "象曰": "泣血涟如。何可长也",
      "时运": "前无去路，保命为要",
      "财运": "犹豫不决，尽失良机",
      "家宅": "家道中落",
      "身体": "呕血之症，凶"
    }
  ]
}