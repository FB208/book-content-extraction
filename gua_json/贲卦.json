{
  "卦名": "贲卦",
  "卦辞": "亨。小利有攸往。",
  "象曰": "山下有火，贲。君子以明庶政，无敢折狱。",
  "时运": "上有阻力，不可任意。",
  "财运": "经理精明，须防套牢。",
  "家宅": "小心火灾。",
  "身体": "郁火上升，慎用寒剂。",
  "动爻": [
    {
      "爻位": "初九",
      "爻辞": "贲其趾，舍车而徒。",
      "象曰": "舍车而徒，义弗乘也。",
      "时运": "个性清高，德优于名。",
      "财运": "脚踏实地，虽小亦亨。",
      "家宅": "勤俭起家，知足不辱。",
      "身体": "初起之病，自可痊愈。"
    },
    {
      "爻位": "六二",
      "爻辞": "贲其须。",
      "象曰": "贲其须，与上兴也。",
      "时运": "平平淡淡，依人成事。",
      "财运": "配合富商，必可获利。",
      "家宅": "祖上福泽；婚嫁宜待。",
      "身体": "遵照医嘱。"
    },
    {
      "爻位": "九三",
      "爻辞": "贲如，濡如，永贞吉。",
      "象曰": "永贞之吉，终莫之陵也。",
      "时运": "名利双收，光华润泽。",
      "财运": "财源如水，可保基业。",
      "家宅": "可以久居；百年偕老。",
      "身体": "游泳健身。"
    },
    {
      "爻位": "六四",
      "爻辞": "贲如，皤如，白马翰如，匪寇婚媾。",
      "象曰": "六四当位疑也。匪寇婚媾，终无尤也。",
      "时运": "安分则吉，明年亨通。",
      "财运": "早些售出，获利了结。",
      "家宅": "先有丧事，后有婚事。",
      "身体": "胸中气阻，调节上下。"
    },
    {
      "爻位": "六五",
      "爻辞": "贲于丘园，束帛戋戋。吝，终吉。",
      "象曰": "六五之吉，有喜也。",
      "时运": "虽为正运，勤俭为宜。",
      "财运": "木材丝绸，皆可获利。",
      "家宅": "家风勤俭；贤妇可喜。",
      "身体": "园林修养。"
    },
    {
      "爻位": "上九",
      "爻辞": "白贲，无咎。",
      "象曰": "白贲无咎，上得志也。",
      "时运": "好运已终，恬淡自适。",
      "财运": "直接出售，依然有利。",
      "家宅": "清白高尚。",
      "身体": "清淡解热；可能归天。"
    }
  ]
}