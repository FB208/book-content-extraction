{
  "卦名": "大有卦",
  "卦辞": "元亨",
  "象曰": "火在天上，大有。君子以遏恶扬善，顺天休命。",
  "时运": "亨通无比，大放异彩。",
  "财运": "放手去做，财富自来。",
  "家宅": "积善之家，子孙保之。",
  "身体": "虚火上升，小心诊治。",
  "动爻": [
    {
      "爻位": "初九",
      "爻辞": "无交害，匪咎，艰则无咎。",
      "象曰": "大有初九，无交害也。",
      "时运": "培养实力，等待机会。",
      "财运": "宜守不宜攻。",
      "家宅": "娶妻小心。",
      "身体": "保养为宜。"
    },
    {
      "爻位": "九二",
      "爻辞": "大车以载，有攸往，无咎。",
      "象曰": "大车以载，积中不败也。",
      "时运": "得贵人之助，有发展机会。",
      "财运": "开始涨价，官方采购。",
      "家宅": "婚姻大吉。",
      "身体": "运动健身。"
    },
    {
      "爻位": "九三",
      "爻辞": "公用亨于天子，小人弗克。",
      "象曰": "公用亨于天子，小人害也。",
      "时运": "功名未显，戒慎免咎。",
      "财运": "日夜防范，可脱险获利。",
      "家宅": "勤俭保家；不宜攀结高亲。",
      "身体": "小心保养。"
    },
    {
      "爻位": "九四",
      "爻辞": "匪其彭，无咎。",
      "象曰": "匪其彭，无咎，明辨晰也。",
      "时运": "一举成名。",
      "财运": "物价高涨，可保无咎。",
      "家宅": "有一时振兴之象。",
      "身体": "继续健身。"
    },
    {
      "爻位": "六五",
      "爻辞": "厥孚交如，威如，吉。",
      "象曰": "黄裳元吉，文在中也。",
      "时运": "直上云霄。",
      "财运": "五谷之类的贸易，物价飞升，有官府支持。",
      "家宅": "富贵之家。",
      "身体": "蒙天所召，不吉。"
    },
    {
      "爻位": "上九",
      "爻辞": "自天佑之，吉无不利。",
      "象曰": "亢龙有悔，盈不可久也。",
      "时运": "由满招损，退而自省。",
      "财运": "过盈则必有亏。",
      "家宅": "婚嫁不利。",
      "身体": "命在旦夕。"
    }
  ]
}