{
  "卦名": "涣卦",
  "卦辞": "元亨，利贞。至于八月有凶。",
  "象曰": "风行水上，涣。先王以享于帝立庙。",
  "时运": "时来运转，水到渠成。",
  "财运": "神明保佑，财源流通。",
  "家宅": "祈神得福；自成佳偶。",
  "身体": "病情严重，恐难回天。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "用拯马壮，吉。",
      "象曰": "初六之吉，顺也。",
      "时运": "险中得救，必有后福。",
      "财运": "同事相助，可以得利。",
      "家宅": "新厦可居。",
      "身体": "急治为宜。"
    },
    {
      "爻位": "九二",
      "爻辞": "涣奔其机，悔亡。",
      "象曰": "涣奔其机，得愿也。",
      "时运": "运势顺利，心想事成。",
      "财运": "货物贸易，如愿以偿。",
      "家宅": "先散后聚；女长于男。",
      "身体": "愁眉不展，良医可治。"
    },
    {
      "爻位": "六三",
      "爻辞": "涣其躬，无悔。",
      "象曰": "涣其躬，志在外也。",
      "时运": "勇于赴难，值得尊敬。",
      "财运": "重财轻命，自讨苦吃。",
      "家宅": "出外免祸；守节之志。",
      "身体": "转危为安。"
    },
    {
      "爻位": "六四",
      "爻辞": "涣其群，元吉。涣有丘，匪夷所思。",
      "象曰": "涣其群，元吉，光大也。",
      "时运": "大运亨通，脱困成业。",
      "财运": "散财济危，自成天地。",
      "家宅": "自有可观。",
      "身体": "凝聚元气，病体自愈。"
    },
    {
      "爻位": "九五",
      "爻辞": "涣汗，其大号涣，王居，无咎。",
      "象曰": "王居无咎，正位也。",
      "时运": "中正之位，诸事皆吉。",
      "财运": "言行中庸，利润自来。",
      "家宅": "富贵之家；必得贵婿。",
      "身体": "肝火过旺，调养心气。"
    },
    {
      "爻位": "上九",
      "爻辞": "涣其血，去逖出，无咎。",
      "象曰": "涣其血，远害也。",
      "时运": "运势通达，无需忧心。",
      "财运": "贸易顺利，财源自来。",
      "家宅": "避免冲突；远嫁之象。",
      "身体": "气血郁积，疏通即治。"
    }
  ]
}