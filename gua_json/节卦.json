{
  "卦名": "节卦",
  "卦辞": "亨。苦节不可贞。",
  "象曰": "泽上有水，节。君子以制数度，议德行。",
  "时运": "品行端正，名利自成。",
  "财运": "正派经营，富裕可求。",
  "家宅": "富有之家；婚姻吉祥。",
  "身体": "节制饮食。",
  "动爻": [
    {
      "爻位": "初九",
      "爻辞": "不出户庭，无咎。",
      "象曰": "不出户庭，知通塞也。",
      "时运": "闭门修行，平安是福。",
      "财运": "不利行商，可以开店。",
      "家宅": "可以安居；夫妇得当。",
      "身体": "安居静养。"
    },
    {
      "爻位": "九二",
      "爻辞": "不出门庭，凶。",
      "象曰": "不出门庭凶，失时极也。",
      "时运": "因循自误，坐失良机。",
      "财运": "错过时机，不赚反赔。",
      "家宅": "屋内无人；旷怨难免。",
      "身体": "行走艰难。"
    },
    {
      "爻位": "六三",
      "爻辞": "不节若，则嗟若。无咎。",
      "象曰": "不节之嗟，又谁咎也？",
      "时运": "得而后失，叹息无奈。",
      "财运": "不知守财，咎由自取。",
      "家宅": "先富后贫；先喜后悲。",
      "身体": "饮食不节致病。"
    },
    {
      "爻位": "六四",
      "爻辞": "安节，亨。",
      "象曰": "安节之亨，承上道也。",
      "时运": "平安是福，听命行事。",
      "财运": "安居外地，一切节俭。",
      "家宅": "平安无事；夫妻平顺。",
      "身体": "病由口入。"
    },
    {
      "爻位": "九五",
      "爻辞": "甘节，吉。往有尚。",
      "象曰": "甘节之吉，居位中也。",
      "时运": "苦尽甘来，功名必显。",
      "财运": "贩米外地，获利必丰。",
      "家宅": "正直节俭；百年好合。",
      "身体": "节食可治。"
    },
    {
      "爻位": "上六",
      "爻辞": "苦节，贞凶。悔亡。",
      "象曰": "苦节贞凶，其道穷也。",
      "时运": "不知变通，贫困一生。",
      "财运": "失去良机，怨叹无用。",
      "家宅": "开明为宜；顽固难处。",
      "身体": "病势垂危。"
    }
  ]
}