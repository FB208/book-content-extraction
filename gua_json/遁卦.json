{
  "卦名": "遁卦",
  "卦辞": "亨，小利贞",
  "象曰": "天下有山，遁。君子以远小人，不恶而严",
  "时运": "最好退隐，君子有吉",
  "财运": "物价涨跌，相去甚远",
  "家宅": "须防作祟；婚姻不宜",
  "身体": "避居吉地",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "遁尾，厉。勿用有攸往",
      "象曰": "遁尾之厉，不往何灾也？",
      "时运": "深藏不露，不会有难",
      "财运": "全部出手，可以免灾",
      "家宅": "早迁为宜；婚姻不合",
      "身体": "走路小心"
    },
    {
      "爻位": "六二",
      "爻辞": "执之用黄牛之革，莫之胜说",
      "象曰": "执用黄牛，固志也",
      "时运": "虽有才华，功名难望",
      "财运": "守住本金，脱售不及",
      "家宅": "迁移有利；退婚不易",
      "身体": "运动有恒"
    },
    {
      "爻位": "九三",
      "爻辞": "系遁，有疾厉。畜臣妾，吉",
      "象曰": "系遁之厉，有疾惫也；畜臣妾吉，不可大事也",
      "时运": "急流勇退，可保无害",
      "财运": "当售即售，久留必损",
      "家宅": "急速迁移；娶妻不利",
      "身体": "不可纵欲伤身"
    },
    {
      "爻位": "九四",
      "爻辞": "好遁。君子吉，小人否",
      "象曰": "君子好遁，小人否也",
      "时运": "君子正名，小人盗名",
      "财运": "出货得利，不必迟疑",
      "家宅": "隐居为宜；离婚之忧",
      "身体": "大人可治，小孩危险"
    },
    {
      "爻位": "九五",
      "爻辞": "嘉遁，贞吉",
      "象曰": "嘉遁贞吉，以正志也",
      "时运": "功成身退，值得嘉许",
      "财运": "应变得宜，仍可获利",
      "家宅": "高风可尚；志同为婚",
      "身体": "避开阴邪"
    },
    {
      "爻位": "上九",
      "爻辞": "肥遁，无不利",
      "象曰": "肥遁无不利，无所疑也",
      "时运": "乐天知命，安享天年",
      "财运": "人弃我取，退可得利",
      "家宅": "利于求财；私奔之虞",
      "身体": "过胖虚脱"
    }
  ]
}