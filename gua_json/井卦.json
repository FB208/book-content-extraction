{
  "卦名": "井卦",
  "卦辞": "元亨，利贞。",
  "象曰": "木上有水，井。君子以劳民劝相。",
  "时运": "木水相生，功名有望。",
  "财运": "利大于本，自然可喜。",
  "家宅": "修屋防水；阴阳得正。",
  "身体": "肾水过胀，立即调治。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "井泥不食，旧井无禽。",
      "象曰": "井泥不食，下也。旧井无禽，时舍也。",
      "时运": "时过运衰，年老无用。",
      "财运": "货物陈旧，难以售出。",
      "家宅": "荒芜难居；婚姻不成。",
      "身体": "旧症不治。"
    },
    {
      "爻位": "九二",
      "爻辞": "井谷射鮒，甕敝漏。",
      "象曰": "井谷射鮒，无与也。",
      "时运": "得小失大，徒有虚名。",
      "财运": "贪财失利，得不偿失。",
      "家宅": "水不可饮；婚姻不佳。",
      "身体": "下漏之症，求医难治。"
    },
    {
      "爻位": "九三",
      "爻辞": "井渫不食，为我心恻。可用汲，王明，并受其福。",
      "象曰": "井渫不食，行恻也。求王明，受福也。",
      "时运": "有才无命，须待两年。",
      "财运": "贩运不当，无利可图。",
      "家宅": "洗井可饮；二年可成。",
      "身体": "心神不安，井泉可愈。"
    },
    {
      "爻位": "六四",
      "爻辞": "井甃，无咎。",
      "象曰": "井甃无咎，修井也。",
      "时运": "修身立名，可望上达。",
      "财运": "整理旧业，应有利润。",
      "家宅": "修整为宜；尚须待时。",
      "身体": "运动健身。"
    },
    {
      "爻位": "九五",
      "爻辞": "井冽寒泉，食。",
      "象曰": "寒泉之食，中正也。",
      "时运": "品行中正，可望进用。",
      "财运": "财源长远，可以获利。",
      "家宅": "谦让有礼；同甘共苦。",
      "身体": "外寒内热，寒剂可解。"
    },
    {
      "爻位": "上六",
      "爻辞": "井收勿幕，有孚元吉。",
      "象曰": "元吉在上，大成也。",
      "时运": "功德在世，大受推崇。",
      "财运": "利益会集，可大可久。",
      "家宅": "积善旺家；两性好合。",
      "身体": "即日可愈。"
    }
  ]
}