{
  "卦名": "革卦",
  "卦辞": "亨，利贞。",
  "象曰": "泽中有火，革。君子以治历明时。",
  "时运": "改变之时，顺时而动。",
  "财运": "消耗过多，迁地贸易。",
  "家宅": "小心防火；改娶之象。",
  "身体": "肾水干枯，肝火上升。",
  "动爻": [
    {
      "爻位": "初九",
      "爻辞": "巩用黄牛之革。",
      "象曰": "巩用黄牛，不可以有为也。",
      "时运": "最好固守，再等几年。",
      "财运": "先立基础，勿图更张。",
      "家宅": "新造之屋；待婚三年。",
      "身体": "中腹胀硬，消积健脾。"
    },
    {
      "爻位": "六二",
      "爻辞": "己日乃革之。征吉，无咎。",
      "象曰": "己日革之，行有嘉也。",
      "时运": "配合吉日，建立功名。",
      "财运": "择吉开张，生意畅旺。",
      "家宅": "择日修宅；可称佳偶。",
      "身体": "即将痊愈。"
    },
    {
      "爻位": "九三",
      "爻辞": "征凶贞厉。革言三就，有孚。",
      "象曰": "革言三就，又何之矣？",
      "时运": "再三考虑，可免后患。",
      "财运": "信用良好，才可获利。",
      "家宅": "三迁为宜；三人为媒。",
      "身体": "三日可愈。"
    },
    {
      "爻位": "九四",
      "爻辞": "悔亡，有孚，改命，吉。",
      "象曰": "改命之吉，信志也。",
      "时运": "转换跑道，好运自来。",
      "财运": "重兴旧业，可以得利。",
      "家宅": "整建有利；重婚有利。",
      "身体": "改求良医。"
    },
    {
      "爻位": "九五",
      "爻辞": "大人虎变，未占有孚。",
      "象曰": "大人虎变，其文炳也。",
      "时运": "大运来到，得意非凡。",
      "财运": "订好价钱，自可获利。",
      "家宅": "不利迁动；须防不贞。",
      "身体": "肝火浮动，不药可愈。"
    },
    {
      "爻位": "上六",
      "爻辞": "君子豹变，小人革面。征凶，居贞吉。",
      "象曰": "君子豹变，其文蔚也。小人革面，顺以从君也。",
      "时运": "守成为宜，功成身退。",
      "财运": "名利兼收，知足常乐。",
      "家宅": "居之得安；多求必应。",
      "身体": "静养修心。"
    }
  ]
}