{
  "卦名": "鼎卦",
  "卦辞": "元吉，亨。",
  "象曰": "木上有火，鼎。君子以正位凝命。",
  "时运": "功名日进，贵不可言。",
  "财运": "自然得利，不劳而获。",
  "家宅": "小心火灾；正配内助。",
  "身体": "肝火上冲，顺气以治。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "鼎颠趾，利出否。得妾以其子，无咎。",
      "象曰": "鼎颠趾，未悖也。利出否，以从贵也。",
      "时运": "因祸得福，荣封之喜。",
      "财运": "小损大利，以商为家。",
      "家宅": "改造旧宅；婚嫁可喜。",
      "身体": "使腹下泻，即可痊愈。"
    },
    {
      "爻位": "九二",
      "爻辞": "鼎有实，我仇有疾，不我能即。吉。",
      "象曰": "鼎有实，慎所之也。我仇有疾，终无尤也。",
      "时运": "功名正盛，小心中伤。",
      "财运": "袋中有财，须防盗窃。",
      "家宅": "富家防窃；不宜娶女。",
      "身体": "实热之症。"
    },
    {
      "爻位": "九三",
      "爻辞": "鼎耳革，其行塞，雉膏不食。方雨，亏悔，终吉。",
      "象曰": "鼎耳革，失其义也。",
      "时运": "妄意改变，难免有悔。",
      "财运": "目前滞销，须待三年。",
      "家宅": "须防突变；可能悔婚。",
      "身体": "小心失聪。"
    },
    {
      "爻位": "九四",
      "爻辞": "鼎折足，覆公餗，其形渥，凶。",
      "象曰": "覆公餗，信如何也？",
      "时运": "小损大刑，千万小心。",
      "财运": "有去无回，小心性命。",
      "家宅": "栋折之患；男女足疾。",
      "身体": "足上生疮，难保完整。"
    },
    {
      "爻位": "六五",
      "爻辞": "鼎黄耳金铉。利贞。",
      "象曰": "鼎黄耳，中以为实也。",
      "时运": "守住大贵，自然吉祥。",
      "财运": "信息通达，可获大利。",
      "家宅": "富贵之家；联姻贵族。",
      "身体": "保护胸耳。"
    },
    {
      "爻位": "上九",
      "爻辞": "鼎玉铉，大吉，无不利。",
      "象曰": "玉铉在上，刚柔节也。",
      "时运": "和善待人，无往不利。",
      "财运": "美玉待沽，自然有利。",
      "家宅": "地位甚高；金玉之盟。",
      "身体": "耳痛之症。"
    }
  ]
}