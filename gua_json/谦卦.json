{
  "卦名": "谦卦",
  "卦辞": "亨，君子有终。",
  "象曰": "地中有山，谦。君子以裒多益寡，称物平施。",
  "时运": "劳苦而不夸耀，有功绩依然谦下待人。",
  "财运": "劳苦而不夸耀，有功绩依然谦下待人。",
  "家宅": "劳苦而不夸耀，有功绩依然谦下待人。",
  "身体": "劳苦而不夸耀，有功绩依然谦下待人。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "谦谦君子，用涉大川，吉。",
      "象曰": "谦谦君子，卑以自牧也。",
      "时运": "不与人争，自可无难。",
      "财运": "投资航运业，有利可图。",
      "家宅": "治家以德，平安和睦。",
      "身体": "健康无虞。"
    },
    {
      "爻位": "六二",
      "爻辞": "鸣谦，贞吉。",
      "象曰": "鸣谦贞吉，中心得也。",
      "时运": "声名传开，颇为得意。",
      "财运": "得人呼应，利益自来。",
      "家宅": "家有恒产，亦有善名。",
      "身体": "健康无虞。"
    },
    {
      "爻位": "九三",
      "爻辞": "劳谦君子，有终，吉。",
      "象曰": "劳谦君子，万民服也。",
      "时运": "劳苦有成，晚运亨通。",
      "财运": "基业已成，永保利益。",
      "家宅": "持盈保泰，安居乐业。",
      "身体": "健康无虞。"
    },
    {
      "爻位": "六四",
      "爻辞": "无不利，挥谦。",
      "象曰": "无不利，挥谦，不违则也。",
      "时运": "正当好运，诸事皆吉。",
      "财运": "利益甚多，留些余地。",
      "家宅": "谦和相处，自无不利。",
      "身体": "健康无虞。"
    },
    {
      "爻位": "六五",
      "爻辞": "不富以其邻，利用侵伐，无不利。",
      "象曰": "利用侵伐，征不服也。",
      "时运": "振作自强，不可姑息。",
      "财运": "防人分取，更生事端。",
      "家宅": "择邻而处，守望相助。",
      "身体": "健康无虞。"
    },
    {
      "爻位": "上六",
      "爻辞": "鸣谦，利用行师，征邑国。",
      "象曰": "鸣谦，志未得也；可用行师，征邑国也。",
      "时运": "大运已过，心意受阻。",
      "财运": "有名无实，认真整顿。",
      "家宅": "防范邪祟。",
      "身体": "健康无虞。"
    }
  ]
}