{
  "卦名": "兑卦",
  "卦辞": "亨，利贞。",
  "象曰": "丽泽兑。君子以朋友讲习。",
  "时运": "朋友支持，好好珍惜。",
  "财运": "有人扶助，获利不难。",
  "家宅": "友朋同住；因友成亲。",
  "身体": "熟医可治。",
  "动爻": [
    {
      "爻位": "初九",
      "爻辞": "和兑，吉。",
      "象曰": "和兑之吉，行未疑也。",
      "时运": "以和为贵，诸事皆吉。",
      "财运": "秋实可收，自然有利。",
      "家宅": "和乐融融；室家得宜。",
      "身体": "宽心无忧。"
    },
    {
      "爻位": "九二",
      "爻辞": "孚兑，吉。悔亡。",
      "象曰": "孚兑之吉，信志也。",
      "时运": "上下同心，自然吉祥。",
      "财运": "以信为本，可长可远。",
      "家宅": "与邻共富；阴阳相合。",
      "身体": "疑病得解。"
    },
    {
      "爻位": "六三",
      "爻辞": "来兑，凶。",
      "象曰": "来兑之凶，位不当也。",
      "时运": "奔走营求，虽成亦辱。",
      "财运": "无信之商，未来堪虑。",
      "家宅": "去伪存诚；先合后离。",
      "身体": "小心外祸。"
    },
    {
      "爻位": "九四",
      "爻辞": "商兑未宁，介疾有喜。",
      "象曰": "商兑未宁，介疾有喜。",
      "时运": "奋斗将成，斟酌行止。",
      "财运": "忧心之事，商量解决。",
      "家宅": "多疾不安；再三媒说而成。",
      "身体": "心神不安，喜事舒怀。"
    },
    {
      "爻位": "九五",
      "爻辞": "孚于剥，有厉。",
      "象曰": "孚于剥，位正当也。",
      "时运": "居安思危，常得其昌。",
      "财运": "虽有小损，信心仍在。",
      "家宅": "诚信为上。",
      "身体": "皮肤有疾，速治可愈。"
    },
    {
      "爻位": "上六",
      "爻辞": "引兑。",
      "象曰": "上六引兑，未光也。",
      "时运": "靠人扶持，平平之运。",
      "财运": "有人指引，稍有小利。",
      "家宅": "内忧外患；似非正聘。",
      "身体": "化解内邪，才可保全。"
    }
  ]
}