{
  "卦名": "咸卦",
  "卦辞": "亨，利贞。取女吉。",
  "象曰": "山上有泽，咸。君子以虚受人。",
  "时运": "谦虚待人，可保功名。",
  "财运": "转运贩卖，必可图利。",
  "家宅": "知其所止；两性和好。",
  "身体": "虚弱宜补。",
  "动爻": [
    {
      "爻位": "初六",
      "爻辞": "咸其拇。",
      "象曰": "咸其拇，志在外也。",
      "时运": "捷足先登，一举成名。",
      "财运": "货物已办，尚未发行。",
      "家宅": "迁居外地；结亲之始。",
      "身体": "足疾医治。"
    },
    {
      "爻位": "六二",
      "爻辞": "咸其腓，凶，居吉。",
      "象曰": "虽凶居吉，顺不害也。",
      "时运": "退守为要，依人成事。",
      "财运": "不宜行商，可以坐贾。",
      "家宅": "婚事有变，顺守则吉。",
      "身体": "无法步行。"
    },
    {
      "爻位": "九三",
      "爻辞": "咸其股，执其随，往吝。",
      "象曰": "咸其股，亦不处也。志在随人，所执下也。",
      "时运": "最好退守，声名不彰。",
      "财运": "合资困难，主事不力。",
      "家宅": "随人他迁不宜；所适非佳偶。",
      "身体": "勿陷情欲。"
    },
    {
      "爻位": "九四",
      "爻辞": "贞吉悔亡，憧憧往来，朋从尔思。",
      "象曰": "贞吉悔亡，未感害也。憧憧往来，未光大也。",
      "时运": "功名显达，不正则恶。",
      "财运": "见利忘义，争夺不休。",
      "家宅": "谨慎交际；须防不贞。",
      "身体": "心神恍惚，最好静养。"
    },
    {
      "爻位": "九五",
      "爻辞": "咸其脢，无悔。",
      "象曰": "咸其脢，志未也。",
      "时运": "所求未成，稍待时日。",
      "财运": "已有感应，不必担心。",
      "家宅": "可保平安。",
      "身体": "有病将愈，且可增寿。"
    },
    {
      "爻位": "上六",
      "爻辞": "咸其辅、颊、舌。",
      "象曰": "咸其辅、颊、舌，滕口说也。",
      "时运": "口才过人，心存正直。",
      "财运": "口舌之祸，不可不慎。",
      "家宅": "口角之争；媒人夸张。",
      "身体": "胡言乱语，祷之求安。"
    }
  ]
}